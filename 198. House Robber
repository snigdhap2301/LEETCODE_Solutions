class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums) == 1:
            return nums[0]

        prevPrev, prev = nums[0], max(nums[0], nums[1])

        for i in range(2, len(nums)):
            current = max(prevPrev + nums[i], prev)
            prevPrev, prev = prev, current

        return prev
