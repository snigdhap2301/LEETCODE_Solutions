class Solution(object):
    def searchMatrix(self, matrix, target):
        """
        :type matrix: List[List[int]]
        :type target: int
        :rtype: bool
        """
        r=0
        for i in range(len(matrix)):
            if target==matrix[i][-1]:
                return True
            if target<matrix[i][-1]:
                r=i
                break
        for i in range(len(matrix[0])):
            if target==matrix[r][i]:
                return True
        return False 
        
