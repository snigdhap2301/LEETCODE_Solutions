class Solution(object):
    def eraseOverlapIntervals(self, intervals):
        """
        :type intervals: List[List[int]]
        :rtype: int
        """
        if not intervals:
            return 0
        intervals.sort(key=lambda x: x[1])
        ed=intervals[0][1]
        r=0        
        for i in range(1,len(intervals)):
            if intervals[i][0]>=ed:
                ed=intervals[i][1]
            else:
                r+=1
        return r
